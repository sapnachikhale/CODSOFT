{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/Images/(.*)",
      "dest": "/Images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/index.html",
      "dest": "/index.html"
    }
  ],
  "buildCommand": null,
  "cleanUrls": true
}
